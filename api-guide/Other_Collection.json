    "description": "다누리 기타 API 엔드포인트"
  },
  "item": [
    {
      "name": "건강 체크",
      "item": [
        {
          "name": "상태 확인",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/health",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "health"
              ]
            },
            "description": "서버 상태를 확인합니다."
          }
        }
      ]
    },
    {
      "name": "공간 관리",
      "item": [
        {
          "name": "공간 사용 시간 조회",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/space/{{spaceId}}",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "space",
                "{{spaceId}}"
              ]
            },
            "description": "특정 공간의 사용 시간을 조회합니다."
          }
        },
        {
          "name": "공간 사용 정보 조회",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"spaceId\": \"{{spaceId}}\",\n  \"deviceId\": \"{{deviceId}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/space/in-use",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "space",
                "in-use"
              ]
            },
            "description": "특정 공간의 사용 정보를 조회합니다."
          }
        },
        {
          "name": "공간 사용 등록",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"spaceId\": \"{{spaceId}}\",\n  \"deviceId\": \"{{deviceId}}\",\n  \"userCount\": 1\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/space",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "space"
              ]
            },
            "description": "공간 사용을 등록합니다."
          }
        }
      ]
    },
    {
      "name": "아이템 대여",
      "item": [
        {
          "name": "아이템 대여",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"itemId\": \"{{itemId}}\",\n  \"quantity\": 1\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/item/{{usageId}}/rental",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "item",
                "{{usageId}}",
                "rental"
              ]
            },
            "description": "아이템을 대여합니다."
          }
        },
        {
          "name": "아이템 반납",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"returnedQuantity\": 1,\n  \"comment\": \"정상 반납\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/item/{{rentalId}}/return",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "item",
                "{{rentalId}}",
                "return"
              ]
            },
            "description": "대여한 아이템을 반납합니다."
          }
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8080",
      "type": "string"
    }
  ]
} 